<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="view_reservation_data_form_view" model="ir.ui.view">
        <field name="name">reservation.data.form</field>
        <field name="model">reservation.data</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name = "procedure_x"/>
                        <field name = "spoken"/>
                        <field name = "decision_date"/>
                        <field name = "last_date_for_resumption"/>
                        <field name = "resupmtion"/>
                        <field name = "registration_number"/>
                        <field name = "registration_date"/>
                        <field name = "resupmtion_circle"/>
                        <field name = "resupmtion_verdict"/>
                        <field name = "resupmtion_verdict_number"/>
                        <field name = "resupmtion_verdict_date"/>
                        <field name = "resupmtion_verdict_spoken"/>
                    </group>
                </sheet>
            </form>
        </field>
</record>

<record id="view_reservation_data_tree_view" model="ir.ui.view">
        <field name="name">reservation.data.tree</field>
        <field name="model">reservation.data</field>
        <field name="arch" type="xml">
            <tree>
                <field name = "procedure_x"/>
                <field name = "spoken"/>
                <field name = "decision_date"/>
                <field name = "last_date_for_resumption"/>
                <field name = "resupmtion"/>
                <field name = "registration_number"/>
                <field name = "registration_date"/>
                <field name = "resupmtion_circle"/>
                <field name = "resupmtion_verdict"/>
                <field name = "resupmtion_verdict_number"/>
                <field name = "resupmtion_verdict_date"/>
                <field name = "resupmtion_verdict_spoken"/>
            </tree>
        </field>
</record>

<record id="action_reservation_data" model="ir.actions.act_window">
        <field name="name">إجراء احتياطي</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">reservation.data</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
    </record> 

